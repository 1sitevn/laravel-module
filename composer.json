{
  "name": "onesite/laravel-module",
  "description": "Generate a module with model, migration, controller, routes and factory",
  "keywords": [
    "laravel",
    "generators"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "1Site.vn",
      "email": "hotro@1site.vn",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^7.0",
    "illuminate/console": "^7.0",
    "illuminate/filesystem": "^7.0",
    "illuminate/events": "^7.0",
    "illuminate/routing": "^7.0",
    "illuminate/view": "^7.0",
    "illuminate/queue": "^7.0",
    "illuminate/contracts": "^7.0",
    "illuminate/config": "^7.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "OneSite\\Module\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OneSite\\Module\\": [
        "src/",
        "tests/"
      ],
      "Modules\\Admin\\": [
        "tests/dist/modules/admin/app/"
      ],
      "App\\": "tests/dist/app/"
    }
  }
}
